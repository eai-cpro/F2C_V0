{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "CDS Hybrid Debug",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npx",
            "args": [
                "cds",
                "watch",
                "--profile",
                "hybrid"
            ],
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            }
        }
    ]
}